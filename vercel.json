{
  "version": 3,
  "builds": [
    {
      "src": "nuxt.config.*",
      "use": "@vercel/node",
      "config": { "zeroConfig": true }
    },
    {
      "src": ".output/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": ".output/server/index.mjs"
    }
  ]
}
